@using Jmelosegui.Mvc.GoogleMap
@model Domain.Entities.Area

@{
    ViewBag.Title = "ShowOnMap";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Объект № @Model.AreaID, цена - @Model.MonthPrice</h2>

@(
        Html.GoogleMap()
            .Name("map")
            .Height(500)
            .Markers(m => m.Add().Title("Hello World!"))
)


@section scripts
{
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC8dms3PGSUNSNRK6x1flVI0pFmQzO0sSk&libraries=places&sensor=false"></script>
    
    @*@(
    Html.GoogleMap().ApiKey("AIzaSyC8dms3PGSUNSNRK6x1flVI0pFmQzO0sSk")
    )*@
    @(    
    Html.GoogleMap().ScriptRegistrar()
    )
}
    